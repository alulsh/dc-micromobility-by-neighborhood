{
  "author": "Alexandra Ulsh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alulsh/dc-bikeshare-by-neighborhood"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "setupFiles": [
      "./test/test-env.js"
    ],
    "coveragePathIgnorePatterns": [
      "test"
    ]
  },
  "scripts": {
    "test": "tsc -p test/tsconfig.json --noemit & eslint */*.ts && jest",
    "dev": "tsc -p src/tsconfig.json --watch",
    "start": "tsc -p src/tsconfig.json --watch & serve"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.5",
    "@jest/globals": "^27.3.1",
    "@types/jest": "^27.0.3",
    "@types/mapbox-gl": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "serve": "^13.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.8"
  }
}
